{
    "name": "app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "build": "rimraf dist && webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
        "ci": "npm run e2e && npm run test",
        "clean": "rimraf node_modules doc dist && npm cache clean",
        "clean-install": "npm run clean && npm install",
        "clean-start": "npm run clean && npm start",
        "client": "concurrently \"npm run docker-client\" \"npm run delayed-open\"",
        "delayed-open": "sleep 5 && open http://localhost:2368",
        "docker-client": "webpack-dev-server --host=0.0.0.0 --hot --inline  --progress --port 2368 & wait",
        "docker-open-terminal": "docker exec -i ${PWD##*/} /bin/bash",
        "docker-server": "concurrently \"npm run server\" \"npm run docker-client\"",
        "docker-start": "docker-compose --p ${PWD##*/} up",
        "docker-stop": "docker-compose --p ${PWD##*/} stop",
        "docs": "typedoc --options typedoc.json src/app/app.ts",
        "e2e": "protractor",
        "e2e-live": "protractor --elementExplorer",
        "lint": "tsconfig-lint",
        "new-component": "yo ng2-webpack:component",
        "new-directive": "yo ng2-webpack:directive",
        "new-interface": "yo ng2-webpack:interface",
        "new-pipe": "yo ng2-webpack:pipe",
        "new-service": "yo ng2-webpack:service",
        "postinstall": "npm run webdriver-update && typings install",
        "predocker-server": "npm install --quiet",
        "predocker-start": "docker pull cmelion/generator-ng2-webpack:latest",
        "server": "json-server --watch json-server/db.json",
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "test": "karma start",
        "test-watch": "karma start --no-single-run --auto-watch",
        "watch": "webpack --watch --progress --profile",
        "webdriver-start": "webdriver-manager start",
        "webdriver-update": "webdriver-manager update"
    },
    "dependencies": {
        "@angular/common": "2.0.0-rc.3",
        "@angular/compiler": "2.0.0-rc.3",
        "@angular/core": "2.0.0-rc.3",
        "@angular/http": "2.0.0-rc.3",
        "@angular/platform-browser": "2.0.0-rc.3",
        "@angular/platform-browser-dynamic": "2.0.0-rc.3",
        "@angular/router-deprecated": "2.0.0-rc.2",
        "@ngrx/store": "^1.5.0",
        "es6-shim": "^0.35.0",
        "es7-reflect-metadata": "1.6.0",
        "lodash": "^4.12.0",
        "react": "^0.14.7",
        "react-dom": "^0.14.7",
        "react-notification-system": "^0.2.6",
        "reflect-metadata": "0.1.3",
        "rxjs": "5.0.0-beta.6",
        "zone.js": "^0.6.12"
    },
    "devDependencies": {
        "autoprefixer": "^6.3.2",
        "copy-webpack-plugin": "^1.1.1",
        "concurrently": "^2.0.0",
        "css-loader": "^0.23.1",
        "es6-promise-loader": "^1.0.1",
        "exports-loader": "0.6.3",
        "expose-loader": "^0.7.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "html-webpack-plugin": "^2.8.1",
        "imports-loader": "^0.6.5",
        "istanbul-instrumenter-loader": "^0.1.3",
        "jasmine-core": "^2.4.1",
        "json-loader": "^0.5.4",
        "json-server": "^0.8.12",
        "karma": "^0.13.22",
        "karma-coverage": "^0.5.2",
        "karma-jasmine": "^1.0.2",
        "karma-phantomjs-launcher": "^1.0.0",
        "karma-chrome-launcher": "^0.2.1",
        "karma-spec-reporter": "0.0.26",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-story-reporter": "^0.3.1",
        "karma-webpack": "^1.7.0",
        "left-pad": "git://github.com/azer/left-pad.git#bff80e3ef0db0bfaba7698606c4f623433d14355",
        "node-sass": "^3.4.2",
        "null-loader": "git+https://github.com/webpack/null-loader.git",
        "opener": "^1.4.1",
        "phantomjs-polyfill": "0.0.2",
        "phantomjs-prebuilt": "^2.1.7",
        "postcss-loader": "^0.8.0",
        "protractor": "^3.3.0",
        "raw-loader": "0.5.1",
        "rimraf": "^2.5.2",
        "sass-loader": "^3.1.2",
        "style-loader": "^0.13.1",
        "ts-loader": "^0.8.2",
        "tsconfig-lint": "^0.11.0",
        "tslint": "^3.9.0",
        "tslint-loader": "^2.1.4",
        "ts-helpers": "^1.1.1",
        "typedoc": "^0.3.12",
        "typescript": "^1.8.10",
        "typings": "^0.8.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.13.0",
        "webpack-dev-server": "^1.14.1",
        "webpack-hot-middleware": "^2.10.0",
        "webpack-load-plugins": "^0.1.2"
    }
}